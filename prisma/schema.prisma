generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id         String   @id @default(cuid())
  slug       String   @unique
  youtubeUrl String
  image      String?
  published  Boolean  @default(false)
  titleEn    String
  titleFa    String
  descEn     String
  descFa     String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
